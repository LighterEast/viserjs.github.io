<hr />
<div class="case-box" id="caseBox{{i}}">
  <div class="op">
    <span class="index">{{#add i 1}}{{/add}}</span>
    <a class="run" data-index="{{i}}">
      <i class="iconfont icon-yunxing"></i>
      Try
    </a>
  </div>
  <div class="case-demo">
    <div id="example{{i}}"></div>
  </div>
  <div class="case-split"></div>
  <div class="case-code">
    <div class="case-code-detail">
      <pre id="code{{i}}"></pre>
    </div>
  </div>
</div>
