<div class="case-box">
  <div class="case-type">
    <h3>{{name}}</h3>
    <div class="op">
      <a class="run" data-index="{{i}}">
        <i class="iconfont icon-yunxing"></i>
        Try
      </a>
    </div>
  </div>
  <div class="case-demo">
    <div id="example{{i}}"></div>
  </div>
  <div class="case-split"></div>
  <u class="case-code">
    <ul class="case-code-switch">
      <li class="case-code-switch-item"></li>
      <li class="case-code-switch-item"></li>
      <li class="case-code-switch-item"></li>
    </ul>
    <pre class="case-code-detail" id="code{{i}}"></pre>
  </div>
</div>
